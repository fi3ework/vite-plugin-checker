{
  "name": "@vite-plugin-checker/vls",
  "version": "0.0.1",
  "private": true,
  "description": "TypeScript checker for vite-plugin-checker",
  "types": "./dist/esm/main.d.ts",
  "main": "./dist/cjs/main.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/main.d.ts",
      "import": "./dist/esm/main.js",
      "default": "./dist/cjs/main.js"
    }
  },
  "engines": {
    "node": ">=14.16"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "build": "tsup",
    "build:test": "tsup --sourcemap inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fi3ework/vite-plugin-checker.git"
  },
  "keywords": [
    "vite-plugin-checker"
  ],
  "author": "fi3ework <fi3ework@gmail.com>",
  "license": "MIT",
  "bugs": "https://github.com/fi3ework/vite-plugin-checker/issues",
  "homepage": "https://github.com/fi3ework/vite-plugin-checker",
  "dependencies": {
    "vscode-languageclient": "^7.0.0",
    "vscode-languageserver": "^7.0.0",
    "vscode-languageserver-textdocument": "^1.0.1",
    "vscode-uri": "^3.0.8",
    "fast-glob": "^3.2.7",
    "chokidar": "^3.5.1"
  },
  "peerDependencies": {
    "vite-plugin-checker": ">= 0.6.1",
    "vls": "*"
  },
  "devDependencies": {
    "vite-plugin-checker": "workspace:*",
    "vls": "^0.8.1",
    "typescript": "^5.0.4"
  }
}
